<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qing.Dao.FileSrcMapper">







    <!-- 添加-->
    <insert id="addfileSrc" parameterType="fileSrc">
        insert into xin_02.user_file_src(userName,fileSrc) values (#{userName} , #{fileSrc})
    </insert>


    <!-- 删除-->
    <delete id="deletefileSrc" parameterType="int">
        delete from xin_02.user_file_src where fileID =#{fileID}
    </delete>

    <!--查询-->
    <select id="queryFileListByName" resultType="fileSrc">
        select * from xin_02.user_file_src where userName = #{userName}
    </select>



</mapper>
